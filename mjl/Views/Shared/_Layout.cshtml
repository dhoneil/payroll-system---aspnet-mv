<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>MJL System</title>

    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title | MJL System</title>

    <meta content='width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no' name='viewport'>
    <link href="//fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700,300italic,400italic,600italic" rel="stylesheet" />
    @Styles.Render("~/Content/css")
    

    <link href="~/Content/jquery.dataTables.min.css" rel="stylesheet" />
    <link href="~/Content/AdminLTE/AdminLTE.min.css" rel="stylesheet" />
    <link href="~/Content/font-awesome-4.7.0/css/font-awesome.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/ionicons-2.0.1/ionicons.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Scripts/AdminLTE/plugins/morris/morris.css" rel="stylesheet" type="text/css" />
    <link href="~/Scripts/AdminLTE/plugins/jvectormap/jquery-jvectormap-1.2.2.css" rel="stylesheet" type="text/css" />
    <link href="~/Scripts/AdminLTE/plugins/daterangepicker/daterangepicker-bs3.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/AdminLTE/AdminLTE.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/AdminLTE/skins/_all-skins.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Scripts/AdminLTE/plugins/datepicker/datepicker3.css" rel="stylesheet" type="text/css" />
    <link rel="stylesheet" href="~/Scripts/AdminLTE/plugins/iCheck/square/_all.css" />
    <link href="~/Scripts/Confirm/jquery-confirm.css" rel="stylesheet" type="text/css" />
    <link rel="stylesheet" href="~/Scripts/AdminLTE/plugins/iCheck/all.css">
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/jquery-confirm.css" rel="stylesheet" />
    <link href="~/Content/timepicker-master/include/ui-1.10.0/ui-lightness/jquery-ui-1.10.0.custom.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/timepicker-master/jquery.ui.timepicker.css" rel="stylesheet" type="text/css" />
    <link href="~/Scripts/select2.min.css" rel="stylesheet" />

    
</head>
<body class="skin-blue">
    <div class="wrapper">

        @Html.Partial("TopMenu")
        @Html.Partial("LeftMenu")
        <div class="content-wrapper">
            <section class="content-header">
                <h1>
                    @ViewBag.Title
                    <small>@ViewBag.TitleSmall</small>
                </h1>
            </section>
            <section class="content">
                @RenderBody()
            </section>
        </div>
        <footer class="main-footer">
            <div class="pull-right hidden-xs">
                <b>Version</b> 2.0
            </div>
            <strong>Copyright &copy; @DateTime.Now.Year</strong> All rights reserved.
        </footer>
    </div>

    <script type="text/javascript">
            var BASE_URL = @Html.Raw(HttpUtility.JavaScriptStringEncode(Url.Content("~"), true));
    </script>

    


    <!-- The Modal -->
    <div class="modal" id="sessionModal" data-backdrop="static">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <p>Note: Your session is expired you need to login to proceed your transaction.</p>
                </div>
                <!-- Modal body -->
                <div class="modal-body">
                    Modal body..
                </div>

            </div>
        </div>
    </div>

    @Scripts.Render("~/bundles/modernizr")

    @RenderSection("Styles", required: false)
    @Scripts.Render("~/bundles/jquery")




    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/modernizr")
    <script src="~/Scripts/jquery-confirm.js"></script>
    <script src="~/Scripts/jquery.numeric.js"></script>
    <script src='~/Scripts/AdminLTE/plugins/fastclick/fastclick.min.js'></script>
    <script src="~/Scripts/AdminLTE/app.js" type="text/javascript"></script>
    <script src="~/Scripts/AdminLTE/plugins/sparkline/jquery.sparkline.min.js" type="text/javascript"></script>
    <script src="~/Scripts/AdminLTE/plugins/jvectormap/jquery-jvectormap-1.2.2.min.js" type="text/javascript"></script>
    <script src="~/Scripts/AdminLTE/plugins/jvectormap/jquery-jvectormap-world-mill-en.js" type="text/javascript"></script>
    <script src="~/Scripts/AdminLTE/plugins/daterangepicker/daterangepicker.js" type="text/javascript"></script>
    <script src="~/Scripts/AdminLTE/plugins/datepicker/bootstrap-datepicker.js" type="text/javascript"></script>
    <script src="~/Scripts/AdminLTE/plugins/iCheck/icheck.min.js" type="text/javascript"></script>
    <script src="~/Scripts/AdminLTE/plugins/slimScroll/jquery.slimscroll.min.js" type="text/javascript"></script>
    <script src="~/Scripts/AdminLTE/plugins/chartjs/Chart.min.js" type="text/javascript"></script>
    <script src="~/Scripts/Confirm/jquery-confirm.js" type="text/javascript"></script>
    <script type="text/javascript" src="~/Scripts/silver-json-to-html.js"></script>
    <script type="text/javascript" src="~/Scripts/general.js"></script>
    <script type="text/javascript" src="~/Scripts/toDictionary.js"></script>

    <script type="text/javascript" src="~/Content/timepicker-master/include/ui-1.10.0/jquery.ui.core.min.js"></script>
    <script type="text/javascript" src="~/Content/timepicker-master/include/ui-1.10.0/jquery.ui.widget.min.js"></script>
    <script type="text/javascript" src="~/Content/timepicker-master/include/ui-1.10.0/jquery.ui.tabs.min.js"></script>
    <script type="text/javascript" src="~/Content/timepicker-master/include/ui-1.10.0/jquery.ui.position.min.js"></script>

    <script type="text/javascript" src="~/Content/timepicker-master/jquery.ui.timepicker.js"></script>
    <script type="text/javascript" src="~/Scripts/session/session_checker.js"></script>

    <script src="~/Scripts/AdminLTE/plugins/datatables/dataTables.bootstrap.js"></script>
    <script src="~/Scripts/jquery.dataTables.min.js"></script>
    <script src="~/Scripts/jquery.numeric.js"></script>
    <script src="~/Scripts/jquery.printElement.js"></script>
    <script src="~/Scripts/select2.min.js"></script>

    @RenderSection("scripts", required: false)

    @*<style>
        [class^='select2'] {
            border-radius: 0px !important;
        }
    </style>*@

    <script>
        $(document).ready(function(){
            $('.numeric').numeric();
            $('.select2').select2();
        });
    </script>




</body>
</html>
